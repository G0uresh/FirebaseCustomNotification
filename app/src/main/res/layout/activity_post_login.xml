<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_post_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  android:orientation="vertical"
    tools:context="gaureshghadi.com.FirebaseCustomNotification.PostLoginActivity">


    <Button
        android:text="NEWS subscibe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/newssubscribe" />

    <Button
        android:text="NEWS Unsubscibe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/newsunsubscribe" />

    <Button
        android:text="Games Subscribe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sgames" />


    <Button
        android:text="Unsubscribe Games"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ungames" />


    <Button
        android:text="LogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/logout" />

</LinearLayout>
